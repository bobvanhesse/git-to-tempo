{
  "name": "git-to-tempo",
  "version": "1.0.0",
  "description": "Convert your git history to Tempo time tracking logs",
  "keywords": [
    "atlassian",
    "git",
    "logs",
    "tempo",
    "time",
    "tracking"
  ],
  "main": "./dist/index.js",
  "files": [
    "./dist/**/*"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "prepare": "npm run build",
    "start": "ts-node -p ."
  },
  "author": "Bob van Hesse <bob@vanhesse.nl>",
  "license": "UNLICENSED",
  "dependencies": {
    "gitlog": "3.1.2",
    "moment": "2.24.0",
    "ramda": "0.26.1"
  },
  "devDependencies": {
    "@types/ramda": "0.26.21",
    "ts-node": "8.3.0",
    "tslib": "1.10.0",
    "typescript": "3.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bobvanhesse/git-to-tempo.git"
  },
  "bugs": {
    "url": "https://github.com/bobvanhesse/git-to-tempo/issues"
  }
}
